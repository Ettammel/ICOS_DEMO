                                <e-point x="France" y="45"></e-point>
                                <e-point x="Germany" y="30"></e-point>
                                <e-point x="Italy" y="35"></e-point>
                                <e-point x="India" y="25"></e-point>
                            </e-points>
                        </e-series>
                        <e-series name="Pediatrician">
                            <e-points>
                                <e-point x="USA" y="45"></e-point>
                                <e-point x="China" y="55"></e-point>
