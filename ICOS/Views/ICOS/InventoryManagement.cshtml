@using Syncfusion.JavaScript;
@using Syncfusion.JavaScript.DataVisualization;

<body class="BodyPatientStatus">
    
    <div class="container">
        <div class="row">
            <div style="margin-top:10px;font-size:x-large;">Inventory Management</div>

        </div>
        <div class="row">

            <div class="col-lg-6" style="background-color:white;">

                <ej-chart id="container1" can-resize="true" load="loadTheme">
                    <e-primary-x-axis>
                        <e-title text="Countries"></e-title>
                    </e-primary-x-axis>
