<body class="LoginBgcolor">
    <div class="container">
        <br />
        <div class="row">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-md-4">
                            <button class="btn btn-primary"><span class="glyphicon glyphicon-refresh"></span></button>
                            <button class="btn btn-primary">Appt List</button>
                            <button class="btn btn-primary">Waiting List</button>
                        </div>
                        <div class="col-md-4">
                            

                        </div>
                        <div class="col-md-4">
                            <form id="searchpatient">
                                <input type="text" class="pull-right form-control" placeholder="Patient Summary" />
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <ej-schedule id="Schedule1" width="100%" height="525px" current-date="new DateTime(2019, 12, 30)" current-view="Day">
                <e-resources>
                    <e-resource field="DoctorId" title="Owner" name="Doctors" allow-multiple="true">
                        <e-resource-settings datasource="@ViewBag.Doctor" text="Text" id="Id" color="Color">
                        </e-resource-settings>
                    </e-resource>
                </e-resources>
                <e-group resources="@ViewBag.Group"></e-group>
                <e-appointment-settings datasource="@ViewBag.Appoint" id="Id" subject='"Subject"' start-time='"StartTime"' end-time='"EndTime"' description='"Description"' all-day='"AllDay"' recurrence='"Recurrence"' recurrence-rule='"RecurrenceRule"' resource-fields='"DoctorId"'>
                </e-appointment-settings>
            </ej-schedule>
        </div>
        

    </div>
</body>
<link href="~/Style/Icos.css" rel="stylesheet" />